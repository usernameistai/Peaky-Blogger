<% if(success && success.length) { %> 
   <div class="alert alert-success alert-dismissible fade show" role="alert">
        <strong><%= success %></strong>
        <button type="button" class="btn float-end d-flex align-items-center" data-bs-dismiss="alert" aria-label="Close"><i class="fa-solid fa-circle-xmark"></i></button>
    </div>
<% } %>

<% if(error && error.length) {  %>
    <div class="alert alert-danger alert-dimissable fade show " role="alert">
        <strong> <%= error %> </strong>
        <button type="button" class="btn float-end d-flex align-items-center" data-bs-dismiss="alert" aria-label="Close"><i class="fa-solid fa-circle-xmark"></i></button>
    </div>
<% } %>